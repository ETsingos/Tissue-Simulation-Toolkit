  <xs:complexType name="MultiCellDS">
    <xs:annotation>
      <xs:appinfo>
        <ontology_definition>
          <oboInOwl:id>MCDS:0000001</oboInOwl:id>
          <dc:description>Multicellular root element type.</dc:description>
          <rdfs:label>MultiCellDS</rdfs:label>
        </ontology_definition>
      </xs:appinfo>        
    </xs:annotation>
    <xs:sequence>
      <xs:element name="cell_line" type="dcl:cell_line" minOccurs="0" maxOccurs="unbounded" >
        <xs:key name="phenotype_dataset_ID_key">
          <xs:selector xpath="phenotype_dataset"></xs:selector>
          <xs:field xpath="@ID"></xs:field>
        </xs:key>
      </xs:element>
      <xs:element name="metadata" type="meta:metadata" minOccurs="0" />
      <xs:element name="microenvironment" type="menv:microenvironment" minOccurs="0" />
      <!-- To be used for snapshots -->
      <xs:element name="cellular_information" type="cell:cellular_information" minOccurs="0" />
    </xs:sequence>
    <xs:attribute name="version" type="xs:string"/>
    <xs:attribute name="type" type="MCDS_type"/>
  </xs:complexType>
  
  <xs:simpleType name="MCDS_type">
    <xs:restriction base="xs:string">
      <xs:enumeration value="cell_line" />
      <xs:enumeration value="snapshot/simulation" />
      <xs:enumeration value="snapshot/experiment" />
      <xs:enumeration value="snapshot/clinical" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:element name="MultiCellDS" type="MultiCellDS">    
    <xs:annotation>
      <xs:appinfo>
        <xs:appinfo xmlns="">
          <ontology_definition>
            <oboInOwl:id>MCDS:0000001</oboInOwl:id>
            <dc:description>Multicellular root element type.</dc:description>
            <rdfs:label>MultiCellDS</rdfs:label>
          </ontology_definition>
        </xs:appinfo>        
      </xs:appinfo>
    </xs:annotation>
  </xs:element>
  
  

